{
  "@context": {
    "shacl": "http://www.w3.org/ns/shacl#",
    "fnml": "http://semweb.mmlab.be/ns/fnml#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "label": "rdfs:label",
    "rdfs:subClassOf": { "@id": "rdfs:subClassOf", "@type": "@id" },
    "rdfs:range": { "@id": "rdfs:range", "@type": "@id" },
    "rdfs:domain": { "@id": "rdfs:domain", "@type": "@id" },
    "boolean": "http://www.w3.org/2001/XMLSchema#boolean",
    "integer": "http://www.w3.org/2001/XMLSchema#integer",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rr": "http://www.w3.org/ns/r2rml#",
    "rml": "http://semweb.mmlab.be/ns/rml#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "rml:referenceFormulation": {
      "@id": "rml:referenceFormulation",
      "@type": "@id"
    },
    "rr:predicate": { "@id": "rr:predicate", "@type": "@id" },
    "rr:object": { "@id": "rr:object", "@type": "@id" }
  },
  "@graph": [
    {
      "@id": "https://example.com/mapping",
      "@type": "rr:TriplesMap",
      "rml:logicalSource": {
        "@type": "rml:LogicalSource",
        "rml:iterator": "$",
        "rml:referenceFormulation": "http://semweb.mmlab.be/ns/ql#JSONPath",
        "rml:source": "input.json"
      },
      "rr:predicateObjectMap": [
        {
          "@type": "rr:PredicateObjectMap",
          "rr:object": "https://skl.standard.storage/mappings/frameObject",
          "rr:predicate": "rdf:type"
        },
        {
          "@type": "rr:PredicateObjectMap",
          "rr:objectMap": {
            "@type": "rr:ObjectMap",
            "rml:reference": "field",
            "rr:datatype": "http://www.w3.org/2001/XMLSchema#integer"
          },
          "rr:predicate": "https://skl.standard.storage/properties/field"
        }
      ],
      "rr:subjectMap": {
        "@type": "rr:SubjectMap",
        "rr:constant": "https://example.com/mapping/subject"
      }
    }
  ]
}
