{
	"name": "@comake/skl-js-engine",
	"version": "0.2.0",
	"description": "Standard Knowledge Language Javascript Engine",
	"keywords": [
		"skl",
		"linked-data",
		"rdf"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/comake/skl-js-engine.git"
	},
	"bugs": {
		"url": "https://github.com/comake/skl-js-engine/issues"
